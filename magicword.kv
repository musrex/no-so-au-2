#:kivy 2.1.0 

<Root>:
    GridLayout:
        rows: 6
        cols: 1
        size_hint: (0.8, 0.8)
        pos_hint: {'center_y': 0.5, 'center_x': 0.5}

<LoadDialog>:
    FileChooserIconView:
        id: filechooser

        GridLayout:
            Button:
                text: "Cancel"
                on_release: root.cancel()